{
  "engines": {
    "node": ">=22",
    "npm": ">=10.0.0"
  },
  "name": "meta-agent",
  "version": "0.0.1",
  "private": true,
  "description": "meta agent orchestrator for viral content and admin campaigns",
  "keywords": [
    "openai",
    "nestjs",
    "backend",
    "system-control"
  ],
  "homepage": "https://github.com/Alejob60/studious-fiesta#readme",
  "bugs": {
    "url": "https://github.com/Alejob60/studious-fiesta/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alejob60/studious-fiesta.git"
  },
  "license": "Apache-2.0",
  "author": "labscode",
  "type": "commonjs",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "mongodb:init": "node scripts/init-mongodb.js",
    "mongodb:test": "node scripts/test-mongodb.js",
    "redis:test": "node scripts/test-redis.js",
    "recovery:phase1": "node scripts/frontend-recovery-phase1.js",
    "recovery:phase2": "node scripts/agent-orchestrator-recovery-phase2.js",
    "recovery:phase3": "node scripts/agent-recovery-phase3.js",
    "recovery:phase4": "node scripts/monitoring-security-recovery-phase4.js",
    "recovery:summary": "node scripts/full-recovery-summary.js",
    "test:local": "ts-node src/test-local-setup.ts",
    "check:services": "node check-services.js",
    "verify-services": "ts-node verify-services.ts",
    "start-services-docker": "ts-node start-services-docker.ts",
    "test:agents": "ts-node scripts/agent-health-check.ts",
    "test:azure-ad": "ts-node src/scripts/test-azure-ad-auth.ts"
  },
  "dependencies": {
    "@azure/identity": "^4.7.0",
    "@azure/keyvault-secrets": "^4.8.0",
    "@azure/service-bus": "^7.9.5",
    "@azure/storage-blob": "^12.28.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.9",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@types/cheerio": "^0.22.35",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "applicationinsights": "^3.9.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.2",
    "helmet": "^8.1.0",
    "mongoose": "^8.22.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.17.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.26",
    "uuid": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
